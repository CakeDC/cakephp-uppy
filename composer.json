{
    "name": "cakedc/cakephp-uppy",
    "description": "CakeDC Uppy plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.3"        
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Uppy\\": "src"            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Uppy\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
